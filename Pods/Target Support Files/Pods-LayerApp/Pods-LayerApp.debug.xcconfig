FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/LayerKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LayerKit"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Atlas.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/MBProgressHUD.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SVProgressHUD.framework/Headers" -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LayerKit"
OTHER_LDFLAGS = $(inherited) -l"z" -framework "Atlas" -framework "CFNetwork" -framework "LayerKit" -framework "MBProgressHUD" -framework "MobileCoreServices" -framework "SVProgressHUD" -framework "Security" -framework "SystemConfiguration" -framework "UIKit"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-LayerApp
PODS_ROOT = ${SRCROOT}/Pods